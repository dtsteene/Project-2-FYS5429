{
    "input_dim": 128,
    "latent_dim": 3,
    "poly_order": 3,
    "widths": [
        64,
        32
    ],
    "activation": "tanh",
    "weight_initializer": "xavier_uniform",
    "bias_initializer": "zeros",
    "optimizer_hparams": {
        "optimizer": "adam"
    },
    "loss_params": {
        "latent_dim": 3,
        "poly_order": 3,
        "include_sine": false,
        "weights": [
            1,
            0.0001,
            1e-06,
            1e-05
        ]
    },
    "update_mask_every_n_epoch": 500,
    "coefficient_threshold": 0.1
}